{
  "name": "@tianbo/tb-ui",
  "version": "1.0.1",
  "description": "tianboUI",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.conf.js --open",
    "build:all": "cross-env NODE_ENV=production webpack --config config/webpack.pro.conf.js",
    "build:es5": "babel src --out-dir dist/es5 --copy-files",
    "build:scss": "node-sass src/ -o src/",
    "build": "npm run build:scss&&npm run build:es5&&npm run build:all",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "tianbo",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "element-react": "^1.4.34"
  }
}
